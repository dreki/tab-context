{
  "short_name": "Tab Context",
  "name": "Tab Context Chrome Extension",
  "description": "Tab Context is a Chrome extension that allows you to switch between collections of tabs with a single click.",
  "icons": {
    "16": "logo192.png",
    "32": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "./static/js/serviceWorker.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Tab Context"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./static/js/content.js"]
    }
  ]
}
